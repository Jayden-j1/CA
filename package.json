{
  "name": "cultural-awareness-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate --schema=prisma/schema.prisma && next dev --turbopack",
    "db:migrate": "prisma migrate dev --name init --schema=prisma/schema.prisma",
    "db:generate": "prisma generate --schema=prisma/schema.prisma",
    "db:studio": "prisma studio --schema=prisma/schema.prisma",
    "db:reset": "rm -f prisma/dev.db && prisma migrate dev --name init --schema=prisma/schema.prisma && prisma generate --schema=prisma/schema.prisma && ts-node prisma/seed.ts",
    "build": "prisma generate --schema=prisma/schema.prisma && next build --turbopack",
    "start": "next start",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "seed": "ts-node prisma/seed.ts",
    "env-check": "tsx scripts/env-check.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "jest-test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit",
    "e2e": "playwright test",
    "token:maintain": "tsx scripts/passwordResetToken.maintain.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@heroicons/react": "^2.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "@react-email/components": "^0.5.5",
    "@react-email/render": "^1.3.1",
    "@react-hookz/web": "^25.1.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@upstash/redis": "^1.35.4",
    "@vis.gl/react-google-maps": "^1.5.5",
    "bcryptjs": "^3.0.2",
    "framer-motion": "^12.23.12",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "resend": "^6.1.1",
    "stripe": "^18.5.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^30.1.2",
    "jest": "^30.1.3",
    "postcss": "^8.5.6",
    "prisma": "^6.16.2",
    "tailwindcss": "^4.1.13",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
